<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Author</key>
	<string>Mike Meyer</string>
	<key>Builder Version</key>
	<string>8536.25</string>
	<key>CFBundleDisplayName</key>
	<string>Dribbble Retina Toggle</string>
	<key>CFBundleIdentifier</key>
	<string>is.meyer.dribble-retina-toggle</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleShortVersionString</key>
	<string>0.2.1</string>
	<key>CFBundleVersion</key>
	<string>3</string>
	<key>Chrome</key>
	<dict>
		<key>Global Page</key>
		<string>background.html</string>
	</dict>
	<key>Content</key>
	<dict>
		<key>Blacklist</key>
		<array>
			<string>http://dribbble.com/shots/*/attachments/*</string>
		</array>
		<key>Scripts</key>
		<dict>
			<key>End</key>
			<array>
				<string>jquery-1.8.1.min.js</string>
				<string>dribbble-retina.js</string>
			</array>
		</dict>
		<key>Stylesheets</key>
		<array>
			<string>dribbble-retina.css</string>
		</array>
		<key>Whitelist</key>
		<array>
			<string>http://dribbble.com/shots/*</string>
		</array>
	</dict>
	<key>Description</key>
	<string>Adds intelligent retina toggling to Dribbble. Add “_2x” to an 800×600px attachment. This extension will do the rest.</string>
	<key>ExtensionInfoDictionaryVersion</key>
	<string>1.0</string>
	<key>Permissions</key>
	<dict>
		<key>Website Access</key>
		<dict>
			<key>Allowed Domains</key>
			<array>
				<string>dribbble.com</string>
				<string>dribbble.s3.amazonaws.com</string>
			</array>
			<key>Include Secure Pages</key>
			<true/>
			<key>Level</key>
			<string>Some</string>
		</dict>
	</dict>
	<key>Update Manifest URL</key>
	<string>http://mikemeyer.s3.amazonaws.com/dribbble-retina-toggle/update-manifest-safari.plist</string>
	<key>Website</key>
	<string>http://mike.meyer.is/busy-with-side-projects#dribbble-retina-toggle</string>
</dict>
</plist>
